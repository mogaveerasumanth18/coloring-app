{
  "cli": {
    "version": ">= 3.8.1",
    "appVersionSource": "local"
  },
  "build": {
    "production": {
      "channel": "production",
      "distribution": "store",
      "pnpm": "9.12.3",
      "ios": {
        "image": "latest"
      },
      "android": {
        "buildType": "apk",
        "image": "latest"
      },
      "env": {
        "EXPO_NO_DOTENV": "1",
        "APP_ENV": "production",
        "FLIPPER_DISABLE": "1",
        "EXPO_USE_METRO_WORKER_MINIFIER": "1",
        "NODE_OPTIONS": "--max_old_space_size=4096"
      }
    },
    "simple": {
      "distribution": "internal",
      "pnpm": "9.12.3",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_USE_METRO_WORKER_MINIFIER": "1",
        "NODE_OPTIONS": "--max_old_space_size=4096"
      }
    },
    "local": {
      "distribution": "internal",
      "pnpm": "9.12.3",
      "android": {
        "buildType": "apk",
        "credentialsSource": "local"
      },
      "env": {
        "APP_ENV": "development",
        "EXPO_USE_METRO_WORKER_MINIFIER": "1",
        "NODE_OPTIONS": "--max_old_space_size=4096"
      }
    },
    "staging": {
      "channel": "staging",
      "distribution": "internal",
      "pnpm": "9.12.3",
      "ios": {
        "image": "latest"
      },
      "android": {
        "buildType": "apk",
        "image": "latest"
      },
      "env": {
        "APP_ENV": "staging",
        "EXPO_NO_DOTENV": "1",
        "FLIPPER_DISABLE": "1",
        "EXPO_USE_METRO_WORKER_MINIFIER": "1",
        "NODE_OPTIONS": "--max_old_space_size=4096"
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "pnpm": "9.12.3",
      "ios": {
        "image": "latest"
      },
      "android": {
        "image": "latest"
      },
      "env": {
        "APP_ENV": "development",
        "EXPO_NO_DOTENV": "1",
        "EXPO_USE_METRO_WORKER_MINIFIER": "1",
        "NODE_OPTIONS": "--max_old_space_size=4096"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
